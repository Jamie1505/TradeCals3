{
  "cli": {
    "version": ">= 15.0.2",
    "appVersionSource": "remote",
    "requireCommit": false
  },
  "doctor": {
    "reactNativeDirectoryCheck": {
      "listUnknownPackages": false
    }
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
